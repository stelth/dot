Host 192.168.56.100
	CheckHostIP no
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET
	IdentityFile ~/.ssh/id_rsa
	User jcox
	ServerAliveInterval 50

